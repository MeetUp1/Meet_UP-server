{
  "name": "meetup-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/bin/www",
    "prepare": "husky install",
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/bin/www",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongodb": "^5.2.0",
    "mongoose": "^6.2.8",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.37.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.12",
    "prettier": "2.8.7",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write ./src",
      "eslint --fix ./src"
    ]
  }
}
